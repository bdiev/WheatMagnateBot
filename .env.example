# Переменные окружения для WheatMagnateBot

# Токен Discord бота (для команд и взаимодействия)
DISCORD_BOT_TOKEN=ваш_токен_здесь

# ID канала Discord (куда бот пишет и слушает команды)
DISCORD_CHANNEL_ID=ваш_channel_id_здесь

# URL Discord webhook (для уведомлений, опционально)
DISCORD_WEBHOOK_URL=ваш_webhook_url_здесь


# Опционально: отключить бота (true/false)
DISABLE_BOT=false